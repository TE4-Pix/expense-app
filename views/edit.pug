extends layout

block content
  .container
    #rightColumn  
      h2 Entries
      hr
      ul#entries

    #leftColumn
      form(id='formEdit')
        h2 Edit Entry
        hr
        br
        p
          | Title: 
          input(id='title', type='text', name='title')
          br 
          br
          | Price: 
          input(id='price', type='text', name='price')
          br
          br
        input(type='submit', class="button", value='Confirm')
        a(id='addSwitch' href='/') Back

  script(src='/javascripts/addExpense.js')
  script(src='/javascripts/main.js')
  script(src='/javascripts/editExpense.js')